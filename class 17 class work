
  import turtle
  import random
  screen = turtle.Screen()
  screen.bgcolor("black")
  t = turtle.Turtle()
  t.speed(0)
  turtle.colormode(255)
  def draw_art():
    for _ in range(70):
      r = random.randint(0,255)
      g = random.randint(0,255)
      b = random.randint(0,255)
      t.color(r,g,b)
      radius = random.randint(20,100)
      t.circle(radius)


  draw_art()
  turtle.done()
